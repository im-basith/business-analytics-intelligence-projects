{
  // ========================================
  // VS CODE SETTINGS FOR AI-AUGMENTED ANALYTICS
  // Author: Abdul Basith
  // GitHub: im-basith
  // Created: December 29, 2025
  // ========================================
  // ========================================
  // PYTHON CONFIGURATION
  // ========================================
  // Python Interpreter - System Python 3.11.5 with all 62 libraries
  "python.defaultInterpreterPath": "/Library/Frameworks/Python.framework/Versions/3.11/bin/python3",
  // Python Language Server
  "python.languageServer": "Pylance",
  // Linting - Catch errors early
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.lintOnSave": true,
  // Formatting - Consistent code style
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": [
    "--line-length",
    "100"
  ],
  // Auto-import suggestions
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  // Type checking
  "python.analysis.typeCheckingMode": "basic",
  // Inlay hints
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  // ========================================
  // JUPYTER NOTEBOOK SETTINGS
  // ========================================
  // Jupyter configuration
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  // Show cell execution time
  "jupyter.showCellExecutionTime": true,
  // Auto-save notebooks
  "notebook.formatOnSave.enabled": true,
  "notebook.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  // ========================================
  // EDITOR SETTINGS
  // ========================================
  // Auto-save
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  // Format on save
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  // Editor appearance
  "editor.fontSize": 14,
  "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
  "editor.lineHeight": 22,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  // Code suggestions
  "editor.suggestSelection": "first",
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "top",
  // Minimap
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 120,
  // Line numbers
  "editor.lineNumbers": "on",
  "editor.renderWhitespace": "selection",
  // Bracket pair colorization
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  // Word wrap
  "editor.wordWrap": "on",
  "editor.rulers": [
    100
  ],
  // ========================================
  // FILE SETTINGS
  // ========================================
  // Files to exclude from explorer
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/*.pyc": true,
    "**/.pytest_cache": true,
    "**/.ipynb_checkpoints": true,
    "**/metabase.db": true
  },
  // Files to watch for changes
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.venv/**": true,
    "**/__pycache__/**": true
  },
  // Default file associations
  "files.associations": {
    "*.json": "jsonc",
    "*.sql": "sql",
    "*.md": "markdown"
  },
  // Trim trailing whitespace
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  // ========================================
  // GIT / SOURCE CONTROL SETTINGS
  // ========================================
  // Git configuration
  "git.enabled": true,
  "git.autofetch": true,
  "git.autoStash": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  // Show git decorations
  "git.decorations.enabled": true,
  // GitLens settings
  "gitlens.currentLine.enabled": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": true,
  "gitlens.statusBar.enabled": true,
  // GitHub Pull Requests
  "githubPullRequests.queries": [
    {
      "label": "Local Pull Request Branches",
      "query": "default"
    },
    {
      "label": "Waiting For My Review",
      "query": "repo:${owner}/${repository} is:open review-requested:@me"
    },
    {
      "label": "All Open",
      "query": "default"
    }
  ],
  // ========================================
  // TERMINAL SETTINGS
  // ========================================
  // Integrated terminal
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,
  // Terminal behavior
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.scrollback": 10000,
  // Default shell
  "terminal.integrated.defaultProfile.osx": "zsh",
  // Terminal environment
  "terminal.integrated.env.osx": {
    "PATH": "${env:HOME}/.local/bin:${env:PATH}"
  },
  // ========================================
  // SEARCH SETTINGS
  // ========================================
  // Search configuration
  "search.exclude": {
    "**/node_modules": true,
    "**/.venv": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/.ipynb_checkpoints": true,
    "**/metabase.db": true
  },
  // Smart case searching
  "search.smartCase": true,
  // ========================================
  // EXPLORER SETTINGS
  // ========================================
  // File explorer
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "explorer.sortOrder": "type",
  // Open files in existing editor group
  "explorer.openEditors.visible": 1,
  // ========================================
  // WORKBENCH SETTINGS
  // ========================================
  // Color theme (change to your preference)
  "workbench.colorTheme": "Default Dark Modern",
  // Icon theme
  "workbench.iconTheme": "vs-seti",
  // Editor tabs
  "workbench.editor.enablePreview": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 10,
  // Activity bar
  "workbench.activityBar.location": "default",
  // Startup behavior
  "workbench.startupEditor": "welcomePage",
  // ========================================
  // LANGUAGE-SPECIFIC SETTINGS
  // ========================================
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  // ========================================
  // SECURITY & PRIVACY
  // ========================================
  // Telemetry
  "telemetry.telemetryLevel": "off",
  // Security
  "security.workspace.trust.enabled": true,
  // ========================================
  // PROJECT-SPECIFIC PATHS
  // ========================================
  // Python analysis search paths
  "python.analysis.extraPaths": [
    "${workspaceFolder}/1_Scripts",
    "${workspaceFolder}/2_Dashboards",
    "${workspaceFolder}/3_Data"
  ],
  // Auto-discover Python tests
  "python.testing.pytestEnabled": true,
  "python.testing.pytestArgs": [
    "."
  ],
  // ========================================
  // BIGQUERY & GOOGLE CLOUD
  // ========================================
  // Environment variables for Google Cloud
  "terminal.integrated.env.osx": {
    "GOOGLE_APPLICATION_CREDENTIALS": "${env:HOME}/.config/gcloud/application_default_credentials.json",
    "GOOGLE_CLOUD_PROJECT": "claude-workspace-1766309098"
  },
  "accessibility.signals.terminalBell": {
    "sound": "off"
  },
  "terminal.integrated.enableVisualBell": false
}
